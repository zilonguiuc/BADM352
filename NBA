#Remember change the netID (zilongl) into your own netID for the first two lines below
create database  zilongl_nba;
use zilongl_nba;
CREATE TABLE PLAYER (
    `FNAME` VARCHAR(7) ,
    `LNAME` VARCHAR(13) ,
    `POSITION` VARCHAR(3) ,
    `TEAM_NAME` VARCHAR(22) ,
    `TEAM_STATE` VARCHAR(2) ,
    `SALARY` INT
);

